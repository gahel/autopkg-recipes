<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest version of mreg-cli for macOS</string>
    <key>Identifier</key>
    <string>com.github.gahel.download.mreg-cli</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>mreg-cli</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.9</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>GitHubReleasesInfoProvider</string>
            <key>Arguments</key>
            <dict>
                <key>github_repo</key>
                <string>unioslo/mreg-cli</string>
                <key>asset_regex</key>
                <string>mreg-cli-macos-latest-[0-9\.]*</string>
                <key>include_prereleases</key>
                <true/>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%url%</string>
                <key>prefetch_filename</key>
                <true/>
                <key>filename</key>
                <string>mreg-cli</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
        <dict>
            <key>Processor</key>
            <string>FileFinder</string>
            <key>Arguments</key>
            <dict>
                <key>find_method</key>
                <string>glob</string>
                <key>pattern</key>
                <string>%RECIPE_CACHE_DIR%/downloads/mreg-cli-macos-latest-[0-9\.]*</string>
                <key>recurse</key>
                <false/>
            </dict>
            <key>output_variable</key>
            <string>found_file</string>
        </dict>
        <dict>
            <key>Processor</key>
            <string>Processor</string>
            <key>Arguments</key>
            <dict>
                <key>message</key>
                <string>FileFinder output: %found_file%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
                <key>source_path</key>
                <string>%found_file%</string>
                <key>destination_path</key>
                <string>%RECIPE_CACHE_DIR%/downloads/mreg-cli-macos-latest-2</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
